{
  "name": "natours",
  "version": "1.0.0",
  "description": "Landing page for natours",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "compress:css": "node-sass css/style.comp.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KajaGrabowska/Natours-Project.git"
  },
  "author": "Kaja",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KajaGrabowska/Natours-Project/issues"
  },
  "homepage": "https://github.com/KajaGrabowska/Natours-Project#readme",
  "devDependencies": {
    "concat": "^1.0.3",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14"
  }
}
